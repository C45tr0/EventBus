{
	"name": "leo_cron",
	"version": "1.0.0",
	"description": "Triggers bots to run by configuration",
	"main": "index.js",
	"directories": {
		"test": "test"
	},
	"scripts": {
		"test": "nodemon --exec \"mocha --recursive -w\" ",
		"debug": "mocha --recursive"
	},
	"config": {
		"leo": {
			"type": "bot",
			"name": "Leo_core_cron",
			"subscriptionFilter": true,
			"handler": "handler",
			"memory": 256,
			"timeout": 300,
			"cron": "1 minute",
			"aws": {
				"role": {
					"ref": "core.roles.LeoCronRole"
				}
			},
			"build": {
				"include": [
					"node_modules/later"
				]
			}
		}
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"later": "^1.2.0"
	}
}
